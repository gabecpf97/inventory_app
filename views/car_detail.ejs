<div class="detail">
    <h1><%=car.name%></h1>
    <h2><%=car.release_year%></h2>
    <h2><%=car.price%></h2>
    <h2><%=car.number_in_stock%></h2>
    <a href="<%=car.maker.url%>"><%=car.maker.name%></a>
    <a href="<%=car.type.url%>"><%=car.type.name%></a>
    <% if (typeof(car.picture) !== 'undefined') { %>
        <img src="data:image/<%=car.picture.content_type%>;base64,<%=car.picture.data.toString('base64')%>">
    <% } %>
    <p><%=car.description%></p>
    <div class="delete_update">
        <a href="<%=`${car.url}/delete`%>">Delete This Car</a>
        <a href="<%=`${car.url}/update`%>">Update This Car</a>
    </div>
</div>