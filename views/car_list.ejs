<div class="content">
    <h1>All Cars</h1>
    <a href="/car/create">Create car</a>
    <ul class="cars">
        <% car_list.forEach(car => { %>
            <a href="<%=car.url%>"><%= car.name %></a>
            <p><%= car.maker.name %></p>
            <p><%= car.type.name %></p>
            <p><%= car.picture %></p>
            <p><%= car.number_in_stock %></p>
        <% }); %>
    </ul>
</div>